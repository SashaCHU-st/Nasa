{
  "name": "nasa-app",
  "version": "1.0.0",
  "description": "Own simple fullstack project made with React and Express.",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "client": "npm run start --prefix client",
    "server": "npm run start --prefix server",
    "install-server": "npm install --legacy-peer-deps --prefix server",
    "install-client": "npm install --legacy-peer-deps --prefix client",
    "install-all": "npm run install-server && npm run install-client",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "fullstack",
    "react",
    "express"
  ],
  "author": "",
  "license": "ISC",
  "engines": {
    "node": ">=16.0.0 <=22.x"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "concurrently": "^9.1.0"
  },
  "dependencies": {
    "body-parser": "^1.20.3",
    "dotenv": "^10.0.0",
    "mongoose": "^7.8.2",
    "mongoose-unique-validator": "^4.0.1",
    "react-scripts": "^5.0.1"
  }
}
